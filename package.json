{
  "name": "@compeon/monthpicker",
  "version": "0.2.0",
  "description": "Simple monthpicker using react & date-fns",
  "repository": "COMPEON/monthpicker",
  "main": "dist/monthpicker.umd.js",
  "module": "dist/monthpicker.es.js",
  "license": "MIT",
  "keywords": [
    "monthpicker",
    "react",
    "react-monthpicker"
  ],
  "files": [
    "dist"
  ],
  "authors": [
    "Bastian Ahrens <bahrens@compeon.de>",
    "Gerrit Seger <gseger@compeon.de>"
  ],
  "scripts": {
    "compile": "rollup -c",
    "start": "start-storybook -p 9001 -c .storybook",
    "test": "jest --watch"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "styled-components": "^3.2.6"
  },
  "dependencies": {
    "date-fns": "2.0.0-alpha.10",
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.4",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "jest": "^23.1.0",
    "jest-styled-components": "^5.0.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "rollup": "^0.60.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "styled-components": "^3.2.6"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "<rootDir>/jestSetup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jestPlugins.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)spec.js"
    ]
  }
}
